    <!-- Semantic UI stuff here -->
    <script src="/static/semantic/semantic.js"></script>
    <link rel="stylesheet" href="/static/semantic/semantic.css">
    <script type="riot/tag" src="/static/tags/user-menu.tag"></script>
    <script type="riot/tag" src="/static/tags/anon-menu.tag"></script>
    <script type="riot/tag" src="/static/tags/all-user-search.tag"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/riot/3.7.3/riot+compiler.min.js"></script>
          <link rel="stylesheet" href="/static/css/style.css">
    <link rel="stylesheet" href="/static/css/jumbotron-narrow.css">

    {% block extra_head %}
    {% endblock %}

</head>
<body class="template">
{% include "main/base/_nav.html" %}
<br><br><br><br><br><br>
<div class="ui container" id="narrow">
    <!-- App Nav -->
    <div class="ui borderless massive menu">
        <a class="header item">{% block app_nav_header %} {% endblock %}</a>
        <div class="ui right secondary menu">
            {% block app_nav_items %}
            {% endblock %}
        </div>
    </div>
    <!-- End App Nav
    <!-- Announcements -->
    <div class="ui message center aligned grid">
        <h2 class="ui huge header">
            {% block announcements_heading %}{% endblock %}
        </h2>
        <div class="announce_desc">
            <p class="lead">
                {% block announcements_desc %}
                {% endblock %}
            </p>
        </div>
        <br>
        <div class="announce">
            {% block announcements %}
            {% endblock %}
        </div>
    </div>
    <!-- End Announcements -->
    <!-- Content -->
    <div class="ui message">
        {% block content %}
        {% endblock %}
    </div>
    <!-- End content -->
    <div class="ui divider"></div>
    <footer>
        {% block app_footer %}{% endblock %}
    </footer>
</div>
<script>
    riot.mount('user-menu');
    riot.mount('anon-menu');
    riot.mount('all-user-search');
</script>
</body>
<footer>
    {% block base_footer %}{% endblock %}
    {#    <script src="/static/javascripts/ga.js"></script>#}
    {#    <script src="/static/javascripts/growingio.js"></script>#}
</footer>
</html>